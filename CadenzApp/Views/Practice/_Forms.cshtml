<div class="row">
    <div class="container">
        <form>
            <div class="input-field col s12">
                <input id="inputDate" type="text" class="datepicker" required>
                <label for="inputDate">Date <span class="red-text">*</span></label>
            </div>

            <div class="input-field col s12 mt-7">
                <p class="range-field">
                    <input id="inputLength" type="range" min="0" max="5" step="0.1" value="0" required />
                </p>
                <label for="inputLength"style="top:-30px">Length (hours) <span class="red-text">*</span></label>
                @*<p class="range-field">
            <input id="inputLogLength" type="range" required>
            <input type="range" id="test5" min="0" max="100" />
        </p>*@
            </div>

            <div class="input-field col s12">
                <select id="inputInstrument" class="select2 browser-default" data-placeholder="Choose an instrument" required></select>
                <label for="inputInstrument" class="for-select">Instrument <span class="red-text">*</span></label>
            </div>

            <div class="input-field col s12">
                <input id="inputSong" type="text" required>
                <label for="inputSong">Song <span class="red-text">*</span></label>
            </div>

            <div class="input-field col s12">
                <textarea id="inputDescription" type="text" placeholder="Enter any additional comments..."></textarea>
                <label for="inputDescription">Additional Notes</label>
            </div>
        </form>
    </div>
</div>